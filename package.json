{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/**",
    "examples/**"
  ],
  "scripts": {
    "dev": "lerna run dev --scope @inrupt/solid-music",
    "package": "lerna run package",
    "demo:cli": "lerna run demo --scope @inrupt/human-graphql-query-example",
    "test": "jest",
    "postinstall": "npm run build && npm run prepare",
